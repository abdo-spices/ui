{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@duck/typescript-config/base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist",
    "strict": false,
    "skipLibCheck": true,
    "paths": {
      "~/old-registry-ui": [
        "../_oldstuff_refactor/ui/index.ts"
      ],
      "~/*": [
        "./*"
      ]
    }
  },
  "include": [
    ".",
    "../_oldstuff_refactor/default/ui/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
